{
    "name": "aircury/ide-configuration",
    "description": "Set your IDE configuration on a YAML file and share it with your source code. Including servers, deployment, interpreters...",
    "keywords": ["ide", "configuration", "phpstorm"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Diaz",
            "email": "jose@aircury.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aircury\\IDEConfiguration\\": "."
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "aircury/collection": "^1.0@dev",
        "aircury/xml": "^1.0@dev",
        "symfony/console": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/options-resolver": "^3.3",
        "symfony/yaml": "^3.3",
        "webpatser/laravel-uuid": "^3.0",
        "symfony/process": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "source"
        },
        "sort-packages": true,
        "bin-dir": "bin",
        "discard-changes": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "bin": [
        "bin/configure-ide"
    ]
}
